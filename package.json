{
  "name": "react-simple-chatbot",
  "version": "0.0.1",
  "description": "React Simple Chatbot",
  "scripts": {
    "start": "webpack-dev-server --inline --content-base build/",
    "test": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register",
    "test:watch": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register --watch",
    "test:coverage": "nyc npm test",
    "build": "webpack"
  },
  "nyc": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ],
    "extension": [
      ".jsx"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LucasBassetti/react-simple-chatbot"
  },
  "keywords": [
    "react",
    "chat",
    "chatbot",
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LucasBassetti/react-simple-chatbot/issues"
  },
  "homepage": "https://github.com/LucasBassetti/react-simple-chatbot#readme",
  "devDependencies": {
    "babel-core": "^6.7.7",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-react": "^6.11.1",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.8",
    "copy-webpack-plugin": "^2.1.1",
    "enzyme": "^2.8.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "mocha": "^3.2.0",
    "nyc": "^10.2.0",
    "react-addons-test-utils": "^15.5.0",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "write-file-webpack-plugin": "^4.0.0"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "random-id": "0.0.2",
    "react": "^15.0.1",
    "react-dom": "^15.0.1"
  }
}
